<?xml version="1.0"?>
<resource>
  <object class="wxFrame" subclass="wxNodeViewer" name="CmguiNodeViewer">
	  <title>Cmgui Node Viewer</title>
	  <flag>wxCLIP_CHILDREN</flag>	
	  <object class = "wxBoxSizer">
		  <orient>wxVERTICAL</orient>
    	  <object class ="sizeritem">
				<flag>wxEXPAND</flag>	
	 			<object class = "wxPanel" name="NodeTextPanel">
					<size>-1,26</size>
				</object>
		  </object>
		  <object class ="sizeritem">
			  <flag>wxEXPAND</flag>
			  <option>1</option>
	 		  <object class = "wxScrolledWindow" name="VariableViewerPanel" />
				<style>wxHSCROLL | wxVSCROLL</style>
		  </object>
		  <object class ="sizeritem">
			  <flag>wxEXPAND</flag>
			  <option>0</option>
	         <flag>wxALIGN_BOTTOM</flag>
	 		  <object class = "wxBoxSizer">
		  		  <orient>wxHORIZONTAL</orient>
				  <object class ="sizeritem">
					  <object class = "wxButton"  name = "OKButton">
					     <label>OK</label>
							 <size>-1,26</size>
					  </object>
				  </object>
				  <object class ="sizeritem">
					  <object class = "wxButton"  name = "ApplyButton">
					     <label>Apply</label>
							 <size>-1,26</size>
					  </object>
				  </object>
				  <object class ="sizeritem">
					  <object class = "wxButton"  name = "RevertButton">
					     <label>Revert</label>
							 <size>-1,26</size>
					  </object>
				  </object>
				  <object class ="sizeritem">
					  <object class = "wxButton"  name = "CancelButton">
					     <label>Cancel</label>
							 <size>-1,26</size>
					  </object>
				  </object>
			  </object>
		 </object>
	  </object>
  </object>
</resource>