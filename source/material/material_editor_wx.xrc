                            	<flag>wxRIGHT|wxALIGN_RIGHT|wxALIGN_CENTER_VERTICAL|wxADJUST_MINSIZE</flag>
      				  		       <object class="wxPanel" name="MaterialEditorSecondTextureChooserPanel">
										<size>150,30</size>
