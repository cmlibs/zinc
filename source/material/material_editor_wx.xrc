				    </object>
		        </object>
 		        <object class ="sizeritem">
               	<option>1</option>
                	<object class="wxButton" name="wxMaterialRevertButton">
				    	<label>Revert</label>
				  	</object>
		        </object>
 		        <object class ="sizeritem">
               	<option>1</option>
                	<object class="wxButton" name="wxMaterialCancelButton">
				    	<label>Cancel</label>
				  	</object>
		        </object>
           </object>	
        </object>


	</object>
</object>
