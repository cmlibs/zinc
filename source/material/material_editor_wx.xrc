 		           			<object class ="sizeritem">
                             	<option>1</option>
                            	<flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL|wxADJUST_MINSIZE</flag>
      				  		       <object class="wxCheckBox" name = "MaterialEditorThirdTextureCheckBox">
				    	   				<label>Third Texture</label>
										<size>-1,30</size>
				       			</object>
								</object>
